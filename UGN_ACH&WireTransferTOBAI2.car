				</mapping>
				<mapping type="formula">
					<source-field><![CDATA["2"]]></source-field>
					<destination-field>File Header.Version Number</destination-field>
				</mapping>
				<mapping type="formula">
					<source-field><![CDATA["071925444"]]></source-field>
					<destination-field>Group.Group Header.Originator Identification</destination-field>
				</mapping>
				<mapping type="formula">
					<source-field><![CDATA["1"]]></source-field>
					<destination-field>Group.Group Header.Group Status</destination-field>
				</mapping>
				<mapping type="formula">
					<source-field><![CDATA["240510"]]></source-field>
					<destination-field>Group.Group Header.As-of-Date</destination-field>
				</mapping>
				<mapping type="formula">
					<source-field><![CDATA[if(Records.$size > 0) {
    def groupedElements = SecGroup(Records, "account");
    //this loop is for creating the 03 record based on account number.
    for(def i = 0; i < groupedElements.Length(); ++i) {
        //Creating Account Section 
        def acc = SecAddElement(self.Group.Account);
        //Creating Account_Identifier_and_Summary_Status section
        def accident = SecAddElement(acc.Account_Identifier_and_Summary_Status);
        accident.Customer_Account_Number = groupedElements[i].account;
        accident.Currency_Code = "USD";
        //this loop is for adding the transaction detail.
        for(def j = 0; j < groupedElements[i].accountGroup.Length(); ++j) {
            def TransDetail = SecAddElement(acc.Transaction_Detail);
            TransDetail.Type_Code = groupedElements[i].accountGroup[j].bai_code;
            TransDetail.Amount = groupedElements[i].accountGroup[j].amount;
            TransDetail.Funds_Type = "0";
            if(groupedElements[i].accountGroup[j].bk_ref.IsNotNull()) {
                TransDetail.Bank_Reference_Number = groupedElements[i].accountGroup[j].bk_ref;
            }
            if(groupedElements[i].accountGroup[j].cust_ref.IsNotNull()) {
                TransDetail.Customer_Reference_Number = groupedElements[i].accountGroup[j].cust_ref;
            }
            def concatingText;
            //this loop is for concating the value from trans_desc1 to trans_desc15 if it present.
            for(def transDesc = 1; transDesc < 15; ++transDesc) {
                if(GetString(groupedElements[i].accountGroup[j], "tran_desc" + transDesc).Length() > 
                     0) {
                    concatingText = concatingText + 
                                      (GetString(groupedElements[i].accountGroup[j], 
                                                 "tran_desc" + transDesc));
                }
            }
            if(concatingText.Length() > 0) {
                TransDetail.Text = concatingText;
            }
        }
        def accTrail = SecAddElement(acc.Account_Trailer);
        accTrail.Account_Control_Total = "settingConstantValue";
    }
}]]></source-field>
					<destination-field>Group.Account</destination-field>
				</mapping>
