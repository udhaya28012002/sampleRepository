			<datavalidationrules>
				<fieldvalidations>
					<fieldvalidation>
						<name>E1</name>
						<fieldname>Group.Account.Transaction Detail</fieldname>
						<formula><![CDATA[def retVal = true;
def parent = $value.TransactionDetailFundsTypeChoice;

if (isNull($value.Funds_Type) || !In ($value.Funds_Type, "V", "S", "D"))
	retVal = (parent.$size == 0);
else {
	retVal = (parent.$size != 0);
	if (retVal) {
		if ($value.Funds_Type == "V") {
			def sec = parent[0].VFundType;
			retVal = (sec.$size > 0) && IsNotNull (sec[0].Value_Date) && IsNotNull (sec[0].Value_Time);
		}
		else if ($value.Funds_Type == "S") {
			def sec = parent[0].SFundType;
			retVal = (sec.$size > 0) && IsNotNull (sec[0].Immediate_Availability) &&
						IsNotNull (sec[0].One_Day_Availability) && IsNotNull (sec[0].Two_Or_More_Days_Availability);
		}
		else if ($value.Funds_Type == "D") {
			def sec = parent[0].DFundType;
			retVal = (sec.$size > 0) && IsNotNull (sec[0].Number_Of_Distributions);
		}
	}
}

return retVal;]]></formula>
						<severity>error</severity>
						<cascade>true</cascade>
						<error-code>E1</error-code>
						<actionmessage><![CDATA["Funds Type is " + $value.Funds_Type + ". Hence, section " + 
	(($value.Funds_Type == "V")? "TransactionDetailFundsTypeChoice.VFundType" :
		($value.Funds_Type == "S")? "TransactionDetailFundsTypeChoice.SFundType" :
			"TransactionDetailFundsTypeChoice.DFundType") + " needs to be populated."]]></actionmessage>
					</fieldvalidation>
				</fieldvalidations>
			</datavalidationrules>
			<tlrvalidationrules>
				<fieldvalidations>
				</fieldvalidations>
			</tlrvalidationrules>
		</validationrules>
